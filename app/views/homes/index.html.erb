<div id="map"></div>

<h1>Tenants</h1>

<div class="row">
  <div class="col-md-6">
    <div class="panel panel-default">

      <div class="panel-heading">
        <h3 class="panel-title text-center">Enter Your Apartment</h3>
      </div>

      <div class="panel-body new-link-section">
        <%= form_for @campaign do |f| %>
          <div class="input-group">
            <%= f.text_field :street_address, id: "street_address", class: "form-control", placeholder: "street address" %><span class="input-group-btn">
            <%= f.button "Submit", class: "btn btn-primary"%></span>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>


<h3>All Campaigns</h3>
<ul class="list-group">
  <% @campaigns.each do |campaign| %>
    <li class="list-group-item"><%= campaign.street_address%></li>
  <% end %>
</ul>
